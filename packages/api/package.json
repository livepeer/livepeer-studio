    "camelcase": "^5.3.1",
    "consul": "^0.39.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^8.1.0",
    "email-validator": "^2.0.4",
    "ethers": "^6.4.0",
    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "express-mung": "^0.5.1",
    "express-prom-bundle": "^6.4.1",
    "fast-stable-stringify": "^1.0.0",
    "fs-extra": "^7.0.1",
    "google-auth-library": "^5.2.2",
    "googleapis": "^43.0.0",
    "http-hash": "^2.0.1",
    "http-proxy": "^1.18.0",
    "http-proxy-middleware": "^0.20.0",
    "is-local-ip": "^1.1.0",
    "isomorphic-webcrypto": "^2.3.8",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "livekit-server-sdk": "^1.1.4",
    "lodash": "^4.17.21",
    "m3u8-parser": "^4.4.0",
    "minio": "^7.0.12",
    "morgan": "^1.9.1",
    "mqtt": "^4.2.6",
    "ms": "^2.1.2",
    "mustache": "^3.0.3",
    "node-fetch": "^2.6.1",
    "node-jose": "^1.1.4",
    "nodemon": "^1.19.0",
    "pg": "^8.3.0",
    "prom-client": "^14.0.1",
    "request-ip": "^2.1.3",
    "source-map-support": "^0.5.19",
    "sql-template-strings": "^2.2.2",
    "string-template": "^1.0.0",
    "stripe": "^8.93.0",
    "tus-node-server": "^0.9.0",
    "uuid": "^9.0.0",
    "whatwg-fetch": "^3.4.0",
