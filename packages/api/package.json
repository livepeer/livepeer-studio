    "istanbul-lib-coverage": "^2.0.5",
    "jest": "^27.5.1",
    "json-schema-to-typescript": "^9.1.1",
    "klaw": "^3.0.0",
    "live-server": "^1.2.1",
    "livereload": "^0.7.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.0.0",
    "parcel": "^1.12.4",
    "pkg": "^5.8.1",
    "prettier": "^2.6.2",
    "redoc-cli": "^0.8.3",
    "tus-js-client": "^3.0.0-0",
    "typescript": "^4.3.4",
    "wrangler": "^0.0.2"
  },
  "homepage": "https://livepeer.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/livepeer/livepeerjs.git"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ]
  },
  "browserslist": [
    "node 10"
  ],
  "alias": {
    "pg-native": "./src/empty.js",
    "level": "./src/empty.js",
    "@kubernetes/client-node": "./src/empty.js"
  },
  "types": "src/schema/types.d.ts",
  "nodemonConfig": {
    "delay": "250"
  },
  "pkg": {
    "targets": [
      "node18-macos-arm64",
      "node18-macos-x64",
      "node18-linux-arm64",
      "node18-linux-x64",
      "node18-windows-x64"
    ],
    "assets": [
      "../www/.next",
      "../www/next.config.js",
      "../../node_modules/dom-helpers/**/*",
      "../../node_modules/react-icons/**/*",
      "../../node_modules/next-mdx-remote/**/*"
    ]
  }
}