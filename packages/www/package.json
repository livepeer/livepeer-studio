    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.mdx?": [
      "prettier --write --parser mdx",
      "git add"
    ]
  },
  "bin": "server.js",
  "pkg": {
    "assets": [
      ".next/**/*",
      "../../node_modules/dom-helpers/**/*",
      "../../node_modules/react-icons/**/*",
      "../../node_modules/next-mdx-remote/**/*"
    ],
    "scripts": [
      ".next/dist/**/*.js"
    ],
    "targets": [
      "node16-macos-arm64",
      "node16-macos-x64",
      "node16-linux-arm64",
      "node16-linux-x64",
      "node16-windows-x64"
    ]
